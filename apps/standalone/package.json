{
  "name": "@flightctl/standalone",
  "version": "0.0.0",
  "description": "Components for Flightctl UI",
  "repository": "https://github.com/flightctl/flightctl-ui.git",
  "homepage": "https://github.com/flightctl/flightctl-ui.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "tsc --noEmit && rimraf dist",
    "build": "NODE_ENV=production npm run ts-node ../../node_modules/.bin/webpack --mode=production",
    "start-prod": "npm run ts-node ../../node_modules/.bin/webpack serve --mode=production --color --progress",
    "dev": "concurrently \"npm run dev:proxy\" \"npm run dev:ui\"",
    "dev:kind": "FLIGHTCTL_SERVER_INSECURE_SKIP_VERIFY='true' FLIGHTCTL_SERVER=https://$(sh ./scripts/get_ext_ip.sh):3443 FLIGHTCTL_CLI_ARTIFACTS_SERVER=http://$(sh ./scripts/get_ext_ip.sh):8090 npm run dev",
    "dev:proxy": "cd ../../proxy && nodemon --watch 'proxy/**/*' --exec 'go run' app.go --signal SIGTERM",
    "dev:ui": "npm run ts-node ../../node_modules/.bin/webpack serve --mode=development --color --progress",
    "lint": "eslint --ext .tsx,.ts ./src/ && prettier --check './src/**/*.{tsx,ts}' && npm run i18n",
    "format": "prettier --check --write './src/**/*.{tsx,ts}'",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'"
  },
  "devDependencies": {
    "@types/react-dom": "^18.2.17",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "postcss": "^8.4.25",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "url-loader": "^4.1.1",
    "webpack": "5.75.0",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@patternfly/react-charts": "^7.2.2",
    "@patternfly/react-core": "^5.0.0 <= 5.4.8",
    "@patternfly/react-icons": "^5.0.0 <= 5.4.2",
    "@patternfly/react-styles": "^5.1.2 <= 5.4.1",
    "@patternfly/react-table": "^5.0.0 <= 5.4.9",
    "formik": "^2.4.5",
    "fuzzysearch": "^1.0.3",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.0",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.51.0",
    "@patternfly/react-code-editor": "^5.4.13",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "^11.7.3",
    "react-router-dom": "^6.22.0",
    "yup": "^1.3.3"
  }
}
