{
  "name": "@flightctl/ocp-plugin",
  "version": "0.0.1",
  "description": "Flightctl OCP plugin",
  "repository": "https://github.com/flightctl/flightctl-ui.git",
  "homepage": "https://github.com/flightctl/flightctl-ui.git",
  "license": "MIT",
  "private": true,
  "consolePlugin": {
    "name": "flightctl-plugin",
    "version": "0.0.1",
    "displayName": "FlightCTL Plugin",
    "description": "FlightCTL Plugin",
    "exposedModules": {
      "FleetsPage": "./src/components/Fleets/FleetsPage.tsx",
      "CreateFleetWizardPage": "./src/components/Fleets/CreateFleetWizardPage.tsx",
      "ImportFleetWizardPage": "./src/components/Fleets/ImportFleetWizardPage.tsx",
      "FleetDetailsPage": "./src/components/Fleets/FleetDetailsPage.tsx",
      "DevicesPage": "./src/components/Devices/DevicesPage.tsx",
      "DeviceDetailsPage": "./src/components/Devices/DeviceDetailsPage.tsx",
      "EditDeviceWizardPage": "./src/components/Devices/EditDeviceWizardPage.tsx",
      "RepositoriesPage": "./src/components/Repositories/RepositoriesPage.tsx",
      "RepositoryDetailsPage": "./src/components/Repositories/RepositoryDetailsPage.tsx",
      "CreateRepositoryPage": "./src/components/Repositories/CreateRepositoryPage.tsx",
      "ResourceSyncToRepositoryPage": "./src/components/ResourceSyncs/ResourceSyncToRepositoryPage.tsx",
      "EnrollmentRequestDetailsPage": "./src/components/EnrollmentRequests/EnrollmentRequestDetailsPage.tsx",
      "appContext": "./src/components/AppContext/AppContext.tsx",
      "OverviewTab": "./src/components/OverviewTab/OverviewTab.tsx"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  },
  "scripts": {
    "prebuild": "tsc --noEmit && rimraf dist",
    "build": "NODE_ENV=production npm run ts-node ../../node_modules/.bin/webpack --mode=production",
    "dev": "IS_OCP_PLUGIN='true' concurrently \"npm run dev:proxy\" \"npm run dev:ocp\" \"npm run dev:ui\"",
    "dev:proxy": "cd ../../proxy && nodemon --watch 'proxy/**/*' --exec 'go run' app.go --signal SIGTERM",
    "dev:ui": "npm run ts-node ../../node_modules/.bin/webpack serve --mode=development --color --progress",
    "dev:ocp": "./start-ocp-console.sh",
    "lint": "eslint --ext .tsx,.ts ./src/ && prettier --check './src/**/*.{tsx,ts}' && npm run i18n",
    "format": "prettier --check --write './src/**/*.{tsx,ts}'",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'"
  },
  "devDependencies": {
    "@types/react-dom": "^18.2.17",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "postcss": "^8.4.25",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.3",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "ts-loader": "^9.4.4",
    "url-loader": "^4.1.1",
    "webpack": "5.75.0",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@openshift-console/dynamic-plugin-sdk": "^1.0.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "^1.0.0",
    "@patternfly/react-core": "^5.0.0 <= 5.4.8",
    "@patternfly/react-icons": "^5.0.0 <= 5.4.2",
    "@patternfly/react-styles": "^5.1.2 <= 5.4.1",
    "@patternfly/react-table": "^5.0.0 <= 5.4.9",
    "@patternfly/react-charts": "7.2.2",
    "@types/react-redux": "^7.1.33",
    "formik": "^2.4.5",
    "fuzzysearch": "^1.0.3",
    "i18next": "^21.8.14",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.51.0",
    "@patternfly/react-code-editor": "^5.4.13",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "^11.7.3",
    "react-redux": "7.2.2",
    "react-router-dom": "^5.3.x",
    "react-router-dom-v5-compat": "6.11.2",
    "yup": "^1.3.3"
  }
}
