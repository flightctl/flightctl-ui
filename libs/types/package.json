{
  "name": "@flightctl/types",
  "version": "0.0.0",
  "description": "Types for Flightctl UI",
  "repository": "https://github.com/flightctl/flightctl-ui.git",
  "homepage": "https://github.com/flightctl/flightctl-ui.git",
  "license": "MIT",
  "private": false,
  "exports": {
    ".": {
      "source": "./index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "index.ts"
  ],
  "scripts": {
    "prebuild": "tsc --noEmit && rimraf dist",
    "build": "tsc --build",
    "gen-types": "node ./scripts/openapi-typescript.mjs && mv tmp-types/types.gen.ts index.ts && npm run build",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.85.1"
  }
}
